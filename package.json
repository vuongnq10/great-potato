{
  "name": "great-potato",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat --network sepolia run contract/scripts/deploy.js",
    "mint": "node contract/scripts/mint.js",
    "copy-json": "node -e \"const fs = require('fs'); const data = fs.readFileSync('./contract/artifacts/contracts/Contract.sol/ERC721NFT.json'); fs.writeFileSync('./src/content/NFT/content/ERC721NFT.json', data);\"",
    "run": "yarn compile && yarn deploy && yarn mint && yarn copy-json"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^0.1.18",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/contracts": "4.9.3",
    "@tanstack/react-query": "^5.8.1",
    "@tanstack/react-query-devtools": "^5.8.2",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@types/socket.io": "^3.0.2",
    "alchemy-sdk": "^3.1.2",
    "bufferutil": "^4.0.8",
    "dotenv": "^16.3.1",
    "encoding": "^0.1.13",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "ethers": "5.7.2",
    "formik": "^2.4.5",
    "fs": "^0.0.1-security",
    "hardhat": "^2.19.0",
    "mongodb": "^6.3.0",
    "next": "^13.5.6",
    "next-connect": "^1.0.0",
    "nodemailer": "^6.9.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "typescript": "^5.3.2",
    "utf-8-validate": "^5.0.10",
    "web3": "^4.2.2",
    "ws": "^8.14.2"
  },
  "browser": {
    "child_process": false,
    "fs/promises": false,
    "net": false,
    "tls": false,
    "dns": false,
    "fs": false
  }
}
